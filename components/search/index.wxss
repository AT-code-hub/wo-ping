.search-bar {
  display: flex;
  background: #3385ff;
  padding: 20rpx 5%;
  flex-flow: column;
  position: fixed;
  width: 90%;
  top: 0;
  left: 0;
  z-index: 99;
}

.search-navigator {
  flex: 1;
  height: 60rpx;
  background: #fff;
  border-radius: 10rpx;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 7%;
}

.search-box {
  flex: 1;
  display: flex;
  background: #3385ff;
}

.search-input {
  flex: 1;
  background: #fff;
  height: 60rpx;
  border-radius: 10rpx;
  padding: 0 20rpx;
  font-size: 30rpx;
}
